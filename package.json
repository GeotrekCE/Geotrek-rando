  "scripts": {
    "postinstall": "npm run build",
    "pretest": "npm run build",
    "test": "gulp tests",
    "prewatch": "npm run build",
    "prestart": "npm run build",
    "start": "gulp watch",
    "dist": "npm run build",
    "upgrade": "git fetch origin && git checkout origin/latest && npm update && npm run dist",



    "build": "run-s files styles scripts",

    "files": "run-p files:*",
    "files:dirs": "mkdirp dist/public/styles dist/public/scripts",
    "files:defaults": "gulp customisation",

    "styles": "run-p styles:*",
    "styles:main": "node-sass src/app/vendors/styles/vendors.scss --output-style compact > dist/public/styles/rando-vendors.css",
    "styles:vendors": "node-sass src/app/rando.scss               --output-style compact > dist/public/styles/rando.css",

    "scripts": "run-p scripts:p scripts:s",

    "scripts:p": "run-p scripts:rando3d",
    "scripts:rando3d": "gulp rando3D",

    "scripts:s": "run-s scripts:translations scripts:main",
    "scripts:translations": "gulp translate",
    "scripts:main": "browserify -r ./node_modules/rando3D/src/app.js:rando3D src/app/app.js -t [ partialify ] -o dist/public/scripts/rando.js"

  }
